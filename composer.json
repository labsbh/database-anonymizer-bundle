{
    "name": "prowebce/database-anonymizer-bundle",
    "type": "symfony-bundle",
    "description": "Database anonymizer bundle.",
    "keywords": [
        "php",
        "database",
        "anonymizer"
    ],
    "homepage": "https://webnet.fr",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/console": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/doctrine-bridge": "^5.0",
        "prowebce/database-anonymizer": "^0.0.6"
    },
    "require-dev": {
        "doctrine/annotations": "^1.12",
        "phpunit/phpunit": "^9.3",
        "fakerphp/faker": "^1.14",
        "symfony/config": "^5.0",
        "symfony/yaml": "^5.0",
        "doctrine/orm": "^2.8",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "doctrine/doctrine-bundle": "^2.2"
    },
    "suggest": {
        "doctrine/annotations": "To enable annotations.",
        "symfony/console": "To enable console commands.",
        "symfony/config": "To configure anonymizer.",
        "symfony/yaml": "To configure anonymizer using yaml files."
    },
    "autoload": {
        "psr-4": {
            "WebnetFr\\DatabaseAnonymizerBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebnetFr\\DatabaseAnonymizerBundle\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "name": "prowebce/database-anonymizer",
            "type": "vcs",
            "url": "https://github.com/labsbh/database-anonymizer.git"
        }
    ]
}
